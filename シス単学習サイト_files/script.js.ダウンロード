const show = document.getElementById("show"),
      field = document.getElementById("field"),
      main = document.getElementById("main"),
      langbtn = document.getElementById("langbtn"),
      number = document.getElementById("number"),
      torf = document.getElementById("TorF"),
      explain = document.getElementById("explain"),
      goresult = document.getElementById("goresult");

let range, que_n, questions, choice = {}, index, choices, score, wrong, mode;

for (let i=1; i<=4; i++) {
  choice[i] = document.getElementById("choice" + i)
}

document.getElementById("langbtn_txt").addEventListener("click", e => {
  if (langbtn.checked) {
    e.target.textContent = "英→日";
  } else {
    e.target.textContent = "日→英";
  };
});

function execute() {
  range = [document.getElementById("from").value,
           document.getElementById("to").value];
  que_n = Math.floor(number.value);
  if (que_n > 1850 || que_n < 1) {
    alert("問題数は1~1850を指定してください");
    return;
  };
  if ((range[1] - range[0]) <= 2) {
    alert("単語番号の範囲の単語は4単語以上にしてください")
    return;
  }
  if (range[0]>range[1] || range[0] < 0 || range[1] < 0 || range[0] > 2027 || range[1] > 2027) {
    alert("単語番号の値がおかしいです");
    return;
  };
  if ((range[1] - range[0] + 1) < que_n) {
    alert("問題数は問題番号の範囲の単語の数より少なくしてください");
    return;
  }
  main.textContent = "　";
  for (let i=1; i<=4; i++) {
    choice[i].textContent = " ";
  };
  torf.textContent = "";
  explain.textContent = "";
  goresult.textContent = "";
  questions = [];
  score = 0;
  wrong = [];
  mode = langbtn.checked ? "toEnglish" : "toJapanese";
  if (mode == "toJapanese") {
    main.style["font-size"] = "100px";
    for (let i=1; i<=4; i++) {
      choice[i].style["font-size"] = "13px";
    }    
  } else {
    main.style["font-size"] = "50px";
    for (let i=1; i<=4; i++) {
      choice[i].style["font-size"] = "20px";
    }
  }
  
  for (let i=1; i<=que_n; i++) {
    let q;
    do {
      q = getRndInteger(Number(range[0]), Number(range[1]));
    } while (questions.indexOf(q) != -1);
    questions.push(q);
  }
  show.textContent = "問題 1/" + que_n;
  ask();
};

function ask() {
  let n;
  index = questions.shift();
  choices = [];
  choices.push(index)
  for (let i=1; i<=3; i++) {
    do {
      n = getRndInteger(Number(range[0]), Number(range[1]));
    } while (choices.indexOf(n) != -1);
    choices.push(n);
  };
  choices = shuffle(choices);
  if (mode == "toJapanese") {
    main.textContent = tango_list[index].en;
  } else {
    main.textContent = tango_list[index].ja;
  };
  if (mode == "toJapanese") {
    for (let i=1; i<=4; i++) {
      choice[i].textContent = tango_list[choices[i-1]].ja;
    }
  } else {
    for (let i=1; i<=4; i++) {
      choice[i].textContent = tango_list[choices[i-1]].en;
    }    
  }
}

function mark(n) {
  if (goresult.innerText) return;
  let correct = choices.indexOf(index);
  if (correct == n){
    const audio = new Audio('https://web.archive.org/web/20250323105619/https://cdn.glitch.global/5c7b5f11-6db6-4f7c-b099-28af974d1416/correct.mp3?v=1707530775248');
    audio.play();
    torf.style.color = "#3feb00";
    torf.textContent = "正解!";
    explain.innerText = "";
    score++
  } else {
    const audio = new Audio('https://web.archive.org/web/20250323105619/https://cdn.glitch.global/5c7b5f11-6db6-4f7c-b099-28af974d1416/wrong.mp3?v=1707530800098');
    audio.play();
    torf.style.color = "#eb0000";
    torf.textContent = "不正解";
    let txt = ""
    txt += main.textContent + " は「" + choice[correct+1].textContent + "」\n"
    txt += choice[n+1].textContent + " は「" + tango_list[choices[n]][mode == "toJapanese" ? "en" : "ja"] + "」";
    explain.innerText = txt;
    wrong.push(main.textContent)
  };
  if (questions.length != 0){
    show.textContent = "問題 " + (que_n - questions.length + 1) + "/" + que_n;
    ask();
  } else {
    goresult.innerText = "終了です。おつかれさまでした\n";
    let btn = document.createElement("button");
    btn.textContent = "結果画面へ";
    btn.addEventListener('click', () => {
      main.textContent = "　";
      for (let i=1; i<=4; i++) {
        choice[i].textContent = " "
      }
      torf.textContent = "";
      explain.textContent = "";
      let txt = "";
      txt += "正解率: " + Math.round((score / que_n) * 100) + "% (" + score + "/" + que_n + ")";
      txt += "\n間違った言葉: ";
      if (wrong.length) {
        for (const s of wrong) {
          txt += (s + ", ")
        }
        txt = txt.slice(0, -2);        
      } else {
        txt += "なし";
      };
      goresult.innerText = txt;
    });
    goresult.appendChild(btn);
  };
};

function btn1() {
  mark(0);
};
function btn2() {
  mark(1);
};
function btn3() {
  mark(2);
};
function btn4() {
  mark(3);
};

function getRndInteger(min, max) {
  max += 1;
  return Math.floor(Math.random() * (max - min) ) + min - 1;
}

function shuffle(arr) {
  var i, j, temp;
  arr = arr.slice();
  i = arr.length;
  if (i === 0) {
    return arr;
  }
  while (--i) {
    j = Math.floor(Math.random() * (i + 1));
    temp = arr[i];
    arr[i] = arr[j];
    arr[j] = temp;
  }
  return arr;
};

